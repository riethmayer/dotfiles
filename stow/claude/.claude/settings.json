{
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/bin/brag-capture-event"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/bin/brag-capture-event"
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/bin/brag-capture-event"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "document-skills@anthropic-skills": true,
    "earlybird-shared@earlybird-marketplace": true,
    "earlybird-product-management@earlybird-marketplace": true,
    "document-skills@anthropic-agent-skills": true
  },
  "extraKnownMarketplaces": {
    "earlybirdvc-skills": {
      "source": {
        "source": "github",
        "repo": "earlybirdvc/skills"
      }
    },
    "anthropic-skills": {
      "source": {
        "source": "github",
        "repo": "anthropics/skills"
      }
    }
  }
}
